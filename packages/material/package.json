{
  "name": "@kit-ui/material",
  "version": "1.0.0",
  "description": "Material Kit use TailwindCSS inspired by Material Design.",
  "main": "./dist/index.js",
  "type": "module",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "!*.test.*",
    "types",
    "*.d.ts",
    "README.md"
  ],
  "scripts": {
    "build-css": "pnpm tailwindcss -i ./styles.css -o ./dist/styles.css -m",
    "build": "swc ./index.js -o dist/index.js && pnpm run build-css"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@swc/cli": "0.3.9",
    "@swc/core": "1.4.0",
    "@types/node": "20.11.16",
    "autoprefixer": "10.4.17",
    "deepmerge": "4.3.1",
    "postcss": "8.4.34",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
